<p id="notice"><%= notice %></p>

<h1>Article News</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @article_news.each do |article_news| %>
      <tr>
        <td><%= article_news.title %></td>
        <td><%= article_news.description %></td>
        <td><%= link_to 'Show', article_news %></td>
        <td><%= link_to 'Edit', edit_article_news_path(article_news) %></td>
        <td><%= link_to 'Destroy', article_news, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Article News', new_article_news_path %>
